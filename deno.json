{
  "tasks": {
    "test": "deno test --unstable -A test/test.ts",
    "bench": "deno run -A --unstable bench/deno.js",
    "bench-node": "node bench/node.mjs",
    "bench-bun": "bun run bench/bun.js"
  },
  "lint": {
    "rules": {
      "exclude": [
        "camelcase",
        "no-explicit-any"
      ],
      "include": [
        "explicit-function-return-type",
        "eqeqeq",
        "explicit-module-boundary-types"
      ]
    }
  }
}
